.match-subject__cd {
  width: 116rpx;
  position: absolute;
  top: 80rpx;
  left: 310rpx;
  height: 116rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.match-subject__cd .text {
  position: absolute;
  text-align: center;
  top: 50%;
  left: 0;
  right: 0;
  transform: translateY(-50%);
  color: #20d071;
  font-size: 48rpx;
  font-weight: 800;
  /* z-index: 99; */
}

.circle-cd {
  width: 58px;
  height: 58px;
  transform: rotate(-90deg);
}

.animation-scale {
  animation: scaleFrames 1s infinite ease-in;
}

.match-main {
  animation: mock 0.5s;
}

.animation-count-down, .animation-circle-count-down {
  animation: mock 1s infinite;
}

@keyframes mock {
  form {
    opacity: 1;
  }

  to {
    opacity: 1;
  }
}

@keyframes scaleFrames {
  0% {
    transform: scale(10) rotate(120deg);
    opacity: 0;
  }

  25% {
    transform: scale(0.7) rotate(0deg);
    opacity: 1;
  }

  30%, 100% {
    transform: scale(1) rotate(0deg);
    opacity: 1;
  }
}

.match-subject__title {
