}

.progress-circle {
  position: relative;
  padding: 0;
  width: 116rpx;
  height: 116rpx;
  /* background-color: #e3e3e3; */
  background: rgba(0, 0, 0, 0.3);
  border-radius: 116rpx;
  transform: rotate(-180deg);
  /* z-index: 99; */
}

.progress-circle::after {
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  right: 10rpx;
  bottom: 10rpx;
  display: block;
  border-radius: 50%;
  /* background: #fff;  */
  background: rgba(0, 0, 0);
  content: " ";
}

.left-half-clipper {
  border-radius: 116rpx;
  width: 116rpx;
  height: 116rpx;
  position: absolute;
  clip: rect(0, 116rpx, 116rpx, 58rpx);
}

.left-half-crow {
  background-color: #000;
  border-radius: 116rpx;
  width: 116rpx;
  height: 116rpx;
  position: absolute;
  clip-path: circle(35% at 50% 50%);
}

.progress-circle.over5 .left-half-clipper {
  clip: rect(auto, auto, auto, auto);
}

.value-bar {
  position: absolute;
  clip: rect(0, 58rpx, 116rpx, 0);
  width: 116rpx;
  height: 116rpx;
  border-radius: 50%;
  border: 16rpx solid #3bc776;
  box-sizing: border-box;
}

.progress-circle.over5 .first50-bar {
  position: absolute;
  clip: rect(0, 116rpx, 116rpx, 58rpx);
  background-color: #3bc776;
  border-radius: 50%;
  width: 116rpx;
  height: 116rpx;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}

.progress-circle .first50-bar {
  display: none;
}

.progress-circle.over5 .first50-bar {
  display: block;
}

.progress-circle .value-bar {
  transition: opacity, transform 0.2s linear;
}

/* Progress bar rotation position */

.progress-circle.p0 .value-bar {
  display: none;
}

.progress-circle.p1 .value-bar {
  transform: rotate(-36deg);
}

.progress-circle.p2 .value-bar {
  transform: rotate(-72deg);
}

.progress-circle.p3 .value-bar {
  transform: rotate(-108deg);
}

.progress-circle.p4 .value-bar {
  transform: rotate(-144deg);
}

.progress-circle.p5 .value-bar {
  transform: rotate(-180deg);
}

.progress-circle.p6 .value-bar {
  transform: rotate(-216deg);
}

.progress-circle.p7 .value-bar {
  transform: rotate(-252deg);
}

.progress-circle.p8 .value-bar {
  transform: rotate(-288deg);
}

.progress-circle.p9 .value-bar {
  transform: rotate(-324deg);
}

.progress-circle.p10 .value-bar {
  transform: rotate(-360deg);
}

