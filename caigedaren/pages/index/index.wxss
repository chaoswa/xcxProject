  top: 0;
}

.store {
  top: 30rpx;
  background: linear-gradient(to bottom right, #a768fd, #860bc6);
}

.store-img image {
  width: 174rpx;
  height: 129rpx;
  margin-left: 130rpx;
}

.store-bg {
  filter: brightness(150%);
}

.exercise {
  width: 334rpx;
  height: 388rpx;
  background: linear-gradient(to bottom right, #e06de5, #ca07fd);
  position: relative;
  top: -150rpx;
  border-radius: 20rpx;
  margin-left: 20rpx;
  overflow: hidden;
}

.exercise-cont {
  margin-left: 30rpx;
  font-size: 30rpx;
}

.exercise-cont text {
  color: #ffde03;
}

.record {
  width: 187rpx;
  height: 189rpx;
