}

.one-top {
  width: 146rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-color: #6b15a9;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
}

.one-bottom {
  width: 146rpx;
  height: 130rpx;
