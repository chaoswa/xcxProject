}

.gold-icon image {
  width: 147rpx;
  height: 144rpx;
  margin-top: -10rpx;
}

.gold-count {
  position: absolute;
  top: 90rpx;
  left: 40rpx;
}

.get-btn {
  width: 400rpx;
  height: 100rpx;
  background-color: #6b15a9;
  color: #fff;
  border-radius: 60rpx;
  text-align: center;
  line-height: 100rpx;
  margin-top: -150rpx;
  font-size: 31rpx;
}

@keyframes circle {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}
