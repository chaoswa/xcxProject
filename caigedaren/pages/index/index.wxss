  z-index: 999;
}

.exercise-bg {
  width: 424rpx;
  height: 463rpx;
  opacity: 0.6;
  position: absolute;
  top: 0;
}

.more-exciting {
  height: 182rpx;
  top: -125rpx;
  background: linear-gradient(to bottom right, #8b1fda, #500488);
}
.more-exciting image{
  width: 334rpx;
  height: 182rpx;
}
.small-title {
  font-size: 32rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
}

.more-img image {
  width: 183rpx;
  height: 81rpx;
  margin: 30rpx 0 0 100rpx;
}

.more-play {
  width: 334rpx;
  height: 182rpx;
  background: linear-gradient(to bottom right, #8b1fda, #500488);
  position: relative;
  top: -125rpx;
  border-radius: 20rpx;
  margin-left: 20rpx;
  overflow: hidden;
}

.play-img image {
  width: 273rpx;
  height: 158rpx;
  position: absolute;
  top: 10rpx;
  left: 20rpx;
}

.souvenir {
  display: flex;
  width: 250rpx;
  height: 100rpx;
  position: fixed;
  left: 0;
  top: 520rpx;
  z-index: 100;
  text-align: center;
  background: linear-gradient(to right, #e06be5, #e215d6);
  border-top-right-radius: 60rpx;
  border-bottom-right-radius: 60rpx;
}

.souvenir-title {
  width: 140rpx;
  height: 100rpx;
  font-size: 30rpx;
  margin-top: 10rpx;
}

.souvenir-img {
  width: 91rpx;
  height: 100rpx;
}

.public {
  width: 207rpx;
  height: 213rpx;
  position: fixed;
  right: 0;
  bottom: -30rpx;
  z-index: 999;
}

.public-img image {
  width: 161rpx;
  height: 153rpx;
  position: absolute;
  top: -120rpx;
  left: 30rpx;
  z-index: -100;
}

.public-title {
  width: 207rpx;
  height: 60rpx;
  background: linear-gradient(to right, #5eb8f6, #6b56fe);
  border-top-left-radius: 60rpx;
  border-bottom-left-radius: 60rpx;
  font-size: 30rpx;
  text-align: center;
  line-height: 60rpx;
  color: #fff;
}

.index-bg {
  width: 100%;
  height: 104rpx;
  position: absolute;
  bottom: -68rpx;
  z-index: -999;
}

.index-bg image {
  width: 750rpx;
  height: 104rpx;
}

.banner {
  width: 100%;
  height: 154rpx;
  position: absolute;
  bottom: -40rpx;
}

.banner image {
  width: 695rpx;
  height: 154rpx;
  margin-left: 27rpx;
}

.login {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1000;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  background: rgba(0, 0, 0, 0.5);
}
.login-bor{
  width: 514rpx;
  height: 393rpx;
  background-color: #fff;
  border-radius: 20rpx;
  display: flex;
  align-items: center;
  flex-flow:column;
  justify-content:space-around;
  margin-top:40%; 
}

.login-tit {
  font-size: 32rpx;
  margin-top: 30rpx;
  color: #000000;
  font-weight: bold;
}
.login-word{
  width: 86%;
  font-size: 26rpx;
  color: #666666;
}
.login-des{
  font-size: 24rpx;
  color:#db3ea8;
}
.user{
  width: 215rpx;
  height: 58rpx;
  color: #fff;
  margin: 0;
  padding: 0;
  border-radius: 60rpx;
  display: inline-block;
}
.login-not{
  width: 215rpx;
  height: 58rpx;
  border-radius: 60rpx;
  display: inline-block;
  margin-right: 20rpx;
}
.login-not image{
 width: 215rpx;
  height: 58rpx;
}
.user image{
  width: 215rpx;
  height: 58rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.mask {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 1000;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* box-sizing: border-box; */
}

.attention {
  width: 600rpx;
  height: 890rpx;
  margin-top: 100rpx;
  position: relative;
}

.attention image {
  width: 600rpx;
  height: 890rpx;
}

.guide {
  animation: upDown 1s ease-in-out alternate infinite;
}

@keyframes upDown {
  0% {
    transform: translateY(20rpx);
  }

  100% {
    transform: translateY(0rpx);
  }
}

.guide image {
  width: 600rpx;
  height: 876rpx;
  margin-top: 50rpx;
}

.btn-attention {
  width: 340rpx;
  height: 80rpx;
  font-size: 30rpx;
  color: #fff;
  line-height: 80rpx;
  text-align: center;
  position: absolute;
  background-color: #e040a7;
  border-radius: 60rpx;
  bottom: 50rpx;
  left: 140rpx;
}

.shortcut {
  width: 402rpx;
  height: 325rpx;
  margin-left: 300rpx;
}

.login-title {
  position: relative;
  z-index: 100;
  margin-top: 60rpx;
}

.login-title image {
  width: 486rpx;
  height: 213rpx;
}

.login-border {
  width: 690rpx;
  height: 687rpx;
  background-color: #f8efff;
  border-radius: 20rpx;
  margin-top: -60rpx;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
}

.login-frame {
  width: 100%;
  height: 380rpx;
  margin: 80rpx auto;
