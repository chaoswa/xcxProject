/**index.wxss**/

page {
  width: 100%;
  /* min-height: 100%; */
  background-color: #410070;
}

.container {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
button::after{
  border: none;
}
navigator {
  display: inline-block;
}

.oneself {
  width: 730rpx;
  height: 385rpx;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
  top: -40rpx;
}

.onself-bor {
  width: 690rpx;
  height: 167rpx;
  position: absolute;
  top: 60rpx;
  left: 20rpx;
  background: rgba(255, 255, 255, 0);
  color: #fff;
  
}
.onself-bor::after{
  border: none;
}
.big-bor {
  width: 130rpx;
  height: 130rpx;
  background-color: #5d9dfe;
  border-radius: 50%;
  margin: 20rpx 0 0 50rpx;
  display: inline-block;
}

.big-bor1 {
  width: 110rpx;
  height: 110rpx;
  background-color: #74baff;
  border-radius: 50%;
  margin: 11rpx auto;
  overflow: hidden;
}

.name {
  display: inline-block;
  position: relative;
  top: -50rpx;
  left: 20rpx;
  font-size: 30rpx;
}

.score {
  display: inline-block;
  width: 200rpx;
  position: relative;
  left: 150rpx;
  top: -48rpx;
}

.score text {
  margin-left: 8rpx;
  font-size: 28rpx;
}

.gold {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
}

.oneself-bg {
  width: 730rpx;
  height: 385rpx;
}

.top {
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
  width: 700rpx;
  height: 900rpx;
  position: relative;
  top: -170rpx;
  /* overflow: hidden; */
}

.top>view {
  display: inline-block;
}

.challenge {
  width: 334rpx;
  height: 388rpx;
  background: linear-gradient(to bottom right, #68d8fd, #3870db);
  border-radius: 20rpx;
  overflow: hidden;
}

.title {
  width: 200rpx;
  font-size: 40rpx;
  margin: 30rpx 0 0 30rpx;
  display: block;
}

.challenge-renwu {
  margin: 10rpx 0 0 20rpx;
  font-size: 31rpx;
}

.challenge-renwu image {
  width: 42rpx;
  height: 34rpx;
  margin-right: 10rpx;
}

.microphone {
  margin-left: 100rpx;
}

.microphone image {
  width: 266rpx;
  height: 347rpx;
}

.challenge-bg {
  width: 334rpx;
  height: 358rpx;
  opacity: 0.5;
  position: absolute;
  top: 0;
}

.rank, .store, .more-exciting {
  width: 334rpx;
  height: 212rpx;
  background: linear-gradient(to bottom right, #8200e0, #4b44cb);
  position: relative;
  border-radius: 20rpx;
  overflow: hidden;
}

.rank-img image {
  width: 177rpx;
  height: 109rpx;
  margin-left: 110rpx;
  margin-top: 30rpx;
}

.rank-bg, .store-bg, .exciting-bg, .play-bg {
