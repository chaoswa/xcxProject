}

.login-one-bor {
  border: 3rpx solid orange;
}

.login-get {
  width: 146rpx;
  height: 180rpx;
  position: absolute;
  top: 0;
  z-index: 100;
  background: rgba(255, 255, 255, 0.5);
}

.login-get image {
  width: 62rpx;
  height: 46rpx;
